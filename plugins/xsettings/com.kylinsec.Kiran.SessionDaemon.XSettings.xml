<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">
    <interface name="com.kylinsec.Kiran.SessionDaemon.XSettings">

        <property name="net_double_click_time" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Maximum time allowed between two clicks for them to be considered a double click (in milliseconds).</doc:para>
                </doc:description>
            </doc:doc>
        </property>
        <property name="net_double_click_distance" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Maximum distance allowed between two clicks for them to be considered a double click (in pixels).</doc:para>
                </doc:description>
            </doc:doc>
        </property>
        <property name="net_dnd_drag_threshold" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Number of pixels the cursor can move before dragging.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_cursor_blink" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether the cursor should blink.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_cursor_blink_time" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Length of the cursor blink cycle, in milleseconds.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_theme_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of theme to use. This is a directory either in ~/.themes (except on Debian), 
                    or the 'system' theme directory, which depends on where GTK was installed. Basically, 
                    it could be anywhere. This should really use the base dir spec or something similar to 
                    locate the theme.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_icon_theme_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of icon theme to use for icons.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_enable_event_sounds" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether to play sounds on user events.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_sound_theme_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>The XDG sound theme to use for event sounds.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="net_enable_input_feedback_sounds" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether to play sounds on input events.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="xft_antialias" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether to antialias Xft fonts; 0=no, 1=yes, -1=default.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="xft_hinting" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether to hint Xft fonts; 0=no, 1=yes, -1=default.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="xft_hint_style" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>What degree of hinting to use; "hintnone", "hintslight", "hintmedium", or "hintfull".</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="xft_rgba" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Type of subpixel antialiasing; "none", "rgb", "bgr", "vrgb", "vbgr".</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="xft_dpi" type="i" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Resolution for Xft, in 1024 * dots/inch. -1 to use default value.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_cursor_theme_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of the cursor theme.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_cursor_theme_size" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Size of the cursor referenced by cursor_theme.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_font_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of default font to use.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_key_theme_name" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of key theme RC file to load.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_toolbar_style" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether default toolbars have text only, text and icons, icons only, etc.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_toolbar_icons_size" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Size of icons in default toolbars.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_im_preedit_style" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>How to draw the input method preedit string.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_im_status_style" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>How to draw the input method statusbar.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_im_module" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Name of the input method module used by GTK+.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_menu_images" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether images should be shown in menus.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_button_images" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether stock icons should be shown in buttons.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_menubar_accel" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>The accelerator for moving the focus to the menubar.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_color_scheme" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>List of symbolic names and color equivalents.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_file_chooser_backend" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Module to use as the filesystem model for the GtkFileChooser widget.  Possible values are "gio" and "gtk+".</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_decoration_layout" type="s" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>This setting determines which buttons should be put in the titlebar of client-side decorated windows, and whether 
                    they should be placed at the left of right. See https://developer.gnome.org/gtk3/stable/GtkSettings.html#GtkSettings--gtk-decoration-layout.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_shell_shows_app_menu" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>This setting determines where application menu will be displayed - in a window or on a panel with MenuModel protocol. 
                    See https://developer.gnome.org/gtk3/stable/GtkSettings.html#GtkSettings--gtk-shell-shows-app-menu.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_shell_shows_menubar" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>This setting determines where window menubars will be displayed - in a window or on a panel with MenuModel protocol.
                    See https://developer.gnome.org/gtk3/stable/GtkSettings.html#GtkSettings--gtk-shell-shows-menubar.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_show_input_method_menu" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether the context menus of entries and text views  should offer to change the input method.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_show_unicode_menu" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether the context menus of entries and text views  should offer to insert control characters.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_automatic_mnemonics" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether mnemonics should be automatically shown and hidden when the user presses the Alt key.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_enable_primary_paste" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para></doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_enable_animations" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether to enable toolkit-wide animations.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="gtk_dialogs_use_header" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>Whether builtin GTK+ dialogs such as the file chooser, the color chooser or the font chooser will use a header bar at the top to show action widgets, or an action area at the bottom. This setting does not affect custom dialogs using GtkDialog directly, or message dialogs.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="window_scaling_factor" type="i" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>This controls the GTK scale factor that maps from window coordinates to the actual device pixels.
                    On traditional systems this is 1, but on very high density displays (e.g. HiDPI, Retina) this can be a higher 
                    value (often 2). Set to 0 to auto-detect.</doc:para>
                </doc:description>
            </doc:doc>
        </property>

        <property name="window_scaling_factor_qt_sync" type="b" access="readwrite">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            <doc:doc>
                <doc:description>
                    <doc:para>This setting determines whether MATE controls the scale factor for QT applications. Enable to 
                    synchronize with the GTK scale factor when initializing the session, disable to control this value elsewhere. 
                    Requires restarting your session.</doc:para>
                </doc:description>
            </doc:doc>
        </property>



    </interface>
</node>
